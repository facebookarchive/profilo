load("//tools/build_defs:fb_python_binary.bzl", "fb_python_binary")
load("//tools/build_defs:fb_python_library.bzl", "fb_python_library")

fb_python_library(
    name = "codegen-lib",
    srcs = glob(["**/*.py"]),
    base_module = "profilo.codegen",
)

fb_python_binary(
    name = "codegen",
    main_module = "profilo.codegen",
    platform = "py2",
    deps = [
        ":codegen-lib",
    ],
)
